{ "parser": "babel-eslint",
  "extends": "standard",
  "env": {
    "es6": true,
    "browser": true
  },
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "rules": {
    "react/jsx-uses-vars": 1,
    "react/jsx-uses-react": 2,
    "semi": [2, "always"]
  }
, "plugins": [
    "react"
  ]
}
